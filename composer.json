{
    "name": "alecgunnar/maverick",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alec Carpenter",
            "email": "alecgunnar@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        
        "symfony/yaml": "^3.1",
        "symfony/config": "^3.1",
        "symfony/dependency-injection": "^3.1",
        "symfony/console": "^3.1",

        "psr/http-message": "^1.0",
        "nikic/fast-route": "^1.0",
        "guzzlehttp/psr7": "^1.3",
        "filp/whoops": "^2.1",
        "container-interop/container-interop": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "mikey179/vfsStream": "^1.6",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Maverick\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "bin": [
        "bin/maverick"
    ],
    "suggest": {
        "twig/twig": "Enables the use of the Twig renderer."
    }
}
